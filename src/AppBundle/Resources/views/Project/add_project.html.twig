{% extends '::base.html.twig' %}

{% block body %}
    <div class="col-md-12">
        <h3>Créer un nouveau projet</h3>
        <form action="" method="POST">
            {{ form_label(form.name, 'Nom du projet') }}
            {{ form_widget(form.name) }}
            {{ form_rest(form) }}
            <br/>
            <button class="btn btn-default" type="submit">Créer</button>
        </form>
    </div>
{% endblock %}